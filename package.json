{
  "name": "csclient",
  "version": "3.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "startrs": "npm start -- --reset-cache",
    "test": "jest",
    "flow": "node_modules/.bin/flow",
    "checkdeps": "ncu --packageFile package.json",
    "i5s": "react-native run-ios --simulator=\"iPhone 5s\"",
    "i6": "react-native run-ios --simulator=\"iPhone 6\"",
    "a": "react-native run-android",
    "ar": "react-native run-android --variant=\"release\"",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "clean:android-win": "cd android/ && gradlew clean && cd ..",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:buildRelease": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "git-hook": "npm run lint -s && npm test -s"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "blueimp-md5": "^2.8.0",
    "fast-html-parser": "^1.0.1",
    "fuse.js": "^3.0.5",
    "he": "^1.1.1",
    "lodash": "^4.17.4",
    "lottie-react-native": "^2.2.0",
    "moment": "^2.18.1",
    "native-base": "^2.3.1",
    "prop-types": "^15.5.10",
    "querystringify": "^1.0.0",
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.1",
    "react-native-bottomsheet": "^1.6.3",
    "react-native-card-view": "^0.0.3",
    "react-native-circle-button": "^1.0.8",
    "react-native-code-push": "^5.0.0-beta",
    "react-native-config": "^0.6.0",
    "react-native-dialog-component": "^0.2.15",
    "react-native-draftjs-render": "^1.0.3",
    "react-native-flip-card": "^3.5.0",
    "react-native-html-parser": "^0.0.5",
    "react-native-htmlview": "^0.12.0",
    "react-native-i18n": "^1.0.0",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-onesignal": "^3.0.5",
    "react-native-parse-html": "^2.2.6",
    "react-native-popup-dialog": "^0.9.33",
    "react-native-shake": "^3.1.0",
    "react-native-snap-carousel": "^3.0.0",
    "react-native-spinkit": "^1.1.1",
    "react-native-vector-icons": "^4.3.0",
    "react-native-zss-rich-text-editor": "^1.0.2",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-form": "^7.0.3",
    "redux-persist": "^4.8.3",
    "redux-saga": "^0.15.6",
    "seamless-immutable": "^7.1.2",
    "shake": "^0.0.9",
    "util": "^0.10.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-flow-react-proptypes": "^4.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^3.0.0",
    "babel-preset-react-native-syntax": "^1.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.2.1",
    "eslint-plugin-react-native": "^3.0.1",
    "jest": "^20.0.4",
    "mockery": "^2.1.0",
    "prettier": "^1.5.3",
    "react-test-renderer": "^16.0.0-alpha.6",
    "reactotron-apisauce": "^1.12.2",
    "reactotron-react-native": "^1.12.2",
    "reactotron-redux": "^1.12.2",
    "reactotron-redux-saga": "^1.12.2",
    "snazzy": "^7.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
